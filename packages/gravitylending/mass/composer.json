{
    "name": "gravitylending/mass",
    "type": "library",
    "description": "A Laravel/Lumen 8 package for Wildfire API",
    "homepage": "https://github.com/gravitylending/mass",
    "license": "proprietary",
    "authors": [
        {
            "name": "TJ Phippen",
            "email": "tphippen@gravitylending.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0",
        "haydenpierce/class-finder": "^0.4.3",
        "mmghv/lumen-route-binding": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "GravityLending\\Mass\\": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gravitylending/mass"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "GravityLending\\Mass\\Providers\\MassServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
